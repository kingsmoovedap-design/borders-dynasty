{
  "name": "@kingsmoovedap-design/borders-dynasty",
  "version": "1.0.0",
  "description": "CodexChain frontend",
  "main": "dist/bundle.js",
  "scripts": {
    "prebuild": "npm run lint && npm test",
    "start": "node dist/bundle.js",
    "dev": "nodemon src/index.js",
    "build": "webpack",
    "test": "node test/api.test.js",
    "test:health": "node test/health.test.js",
    "lint": "eslint .",
    "hardhat": "hardhat",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kingsmoovedap-design/borders-dynasty.git"
  },
  "homepage": "https://github.com/kingsmoovedap-design/borders-dynasty#readme",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "devDependencies": {
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.1",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "hardhat": "^2.22.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/github": "^9.2.5",
    "@semantic-release/git": "^10.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^9.0.11"
  },
  "release": {
    "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": ["CHANGELOG.md", "package.json"],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
"devDependencies": {
  "@babel/core": "^7.24.0",
  "@babel/preset-env": "^7.24.1",
  "@semantic-release/changelog": "^6.0.3",
  "@semantic-release/commit-analyzer": "^11.1.0",
  "@semantic-release/git": "^10.0.1",
  "@semantic-release/github": "^9.2.5",
  "@semantic-release/release-notes-generator": "^12.1.0",
  "babel-loader": "^9.1.3",
  "eslint": "^8.57.0",
  "hardhat": "^2.22.3",
  "html-webpack-plugin": "^5.5.3",
  "husky": "^9.0.11",
  "nodemon": "^3.1.0",
  "webpack": "^5.91.0",
  "webpack-cli": "^5.1.4"
}
